int appendCharacters(string s, string t) {
        int m=0;
        int n=0;
        int count=0;

        while(m<s.size() && n<t.size()){
            if(s[m]==t[n]){
                m++;
                n++;
            }
            else{
                m++;
            }
        }

        return(t.size()-n);
        
